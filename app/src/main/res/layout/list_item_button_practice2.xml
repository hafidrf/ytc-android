<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="data"
            type="com.yureka.technology.ytc.ui.beranda.materi.practice2.model.ItemModel" />
        <variable
            name="viewModel"
            type="com.yureka.technology.ytc.ui.beranda.materi.practice2.Practice2ViewModel" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <androidx.cardview.widget.CardView
            android:id="@+id/cv_practice2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="0dp"
            app:cardCornerRadius="12dp"
            app:cardElevation="1dp"
            app:cardUseCompatPadding="true"
            tools:ignore="MissingConstraints">

            <LinearLayout
                android:id="@+id/ll_button_practice2"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:orientation="vertical"
                android:gravity="center"
                android:background="@drawable/rounded_rectangle_border_practice2_inactice"
                tools:ignore="MissingConstraints">

                <TextView
                    android:id="@+id/tv_jawaban"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/nunito_semibold"
                    android:text="@{data.jawaban}"
                    android:textColor="#8492A6"
                    android:textFontWeight="400"
                    android:textSize="16sp"
                    tools:text="breakfast" />
            </LinearLayout>
        </androidx.cardview.widget.CardView>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>